<script type="text/javascript">
	$(document).ready(function() {

		/*$('input#figuredisplay').switchButton({
			on_label: 'Cumulative discounted net returns, in thousands of dollars per acre, across each year of the vineyard\'s lifetime',
			off_label: 'Yields, in tons per acre, across each year of the vineyard\'s lifetime',
			width: 70,
			height: 20,
			button_width: 40
		});*/

		var storage = $.localStorage;

		$('.submit').each(function() {
			$(this).click(function() {
				if ( $('input[name=yearfig]:checked').length===0 ) {
					$('body,html').stop(true,true).animate({scrollTop: $('#actualRegion').offset().top - $('header').height()}, '500', 'swing');
					$('.select-year').addClass('alert');
					return false;
				} else {
					var theTableInputObject = { 
						'figuredisplay' : $('input[name=figuredisplay]:checked').val(),
						'year3' : $('input[value="Year3"]').is(':checked'),
						'year5' : $('input[value="Year5"]').is(':checked'),
						'year10' : $('input[value="Year10"]').is(':checked'),
						'regionAssumed' : $('select#backupRegion').val(),
						'regionBasis' : $('input#region-basis').val(),
						'discount' : $('input#discount').val(),
						'cost0' : $('input#cost0').val(),
						'cost1' : $('input#cost1').val(),
						'cost2' : $('input#cost2').val(),
						'cost3' : $('input#cost3').val(),
						'pc' : $('input#pc').val(),
						'price' : $('input#price').val(),
						'yield0' : $('input#yield0').val(),
						'yield1' : $('input#yield1').val(),
						'yield2' : $('input#yield2').val(),
						'yield3' : $('input#yield3').val(),
						'yield4' : $('input#yield4').val(),
						'yield5' : $('input#yield5').val()
					}
					storage.set('run-model', theTableInputObject);
				}
			});
		});

		$('#actualRegion').change(function() {
			setRegionalDefaults( $('select#actualRegion').val() );
			$('select#backupRegion').val($('select#actualRegion').val());
		})

		$('#regionalDefaults').click(function() {
			setRegionalDefaults( $('select#backupRegion').val() );
		});
		$('select[name=practice]').change(function() {
			setPracticeCost( $('select#backupRegion').val(), $(this).val() );
			if ($(this).val()==='custom')
				$('input[name=pc]').focus();
		});
		$('input[name=pc]').change(function() {
			setPracticeSelect( $('select#backupRegion').val(), $(this).val() );
		});

		$('#form input[type=text]').change(function() {
			checkRegionDisplay(	$('select#backupRegion').val(),
				$('input#discount').val(),
				$('input#cost0').val(),
				$('input#cost1').val(),
				$('input#cost2').val(),
				$('input#cost3').val(),
				$('input#pc').val(),
				$('input#price').val(),
				$('input#yield0').val(),
				$('input#yield1').val(),
				$('input#yield2').val(),
				$('input#yield3').val(),
				$('input#yield4').val(),
				$('input#yield5').val()
			);
		});

		styleGlossaryLinks();
		focusCustomParameters();
		mobileSubstitutions();
		toggleFormOptions();
		$( '.swipebox' ).swipebox();

		//$("input, select").mouseover(zoomDisable).mousedown(zoomEnable); 

	
	});
</script>

